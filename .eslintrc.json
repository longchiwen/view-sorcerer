{
    "rules": {
        "indent": [
            0,
            4
        ],
        "quotes": [
            0,
            "double"
        ],
        "linebreak-style": [
            1,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars":[
            1, { "args": "all" }
        ],
        "no-console":[
            1
        ],
        "no-debugger":[
            1
        ],
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"]
    },
    "env": {
        "es6": true,
        "node": true,
        "jasmine":true,
        "browser": true
    },
    "extends": "eslint:recommended"
}